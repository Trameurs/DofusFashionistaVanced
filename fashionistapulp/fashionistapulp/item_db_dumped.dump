CREATE TABLE IF NOT EXISTS "items" (
	`id`	INTEGER PRIMARY KEY AUTOINCREMENT,
	`name`	text,
	`level`	INTEGER,
	`type`	INTEGER,
	`item_set`	INTEGER,
	`ankama_id`	INTEGER,
	`ankama_type`	text,
	`removed`	INTEGER,
	`dofustouch`	INTEGER,
	FOREIGN KEY(`type`) REFERENCES item_types ( id ),
	FOREIGN KEY(`item_set`) REFERENCES sets ( id )
);
CREATE TABLE IF NOT EXISTS "sets" (
	`id`	INTEGER PRIMARY KEY AUTOINCREMENT,
	`name`	text,
	`ankama_id`	INTEGER,
	`dofustouch`	INTEGER
);
